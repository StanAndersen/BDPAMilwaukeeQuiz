<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bdpaquiz.andersen.stanford.bdpamilwaukeequiz" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChangeToAnswerScreen"
            android:label="@string/title_activity_change_to_answer_screen"
            android:parentActivityName=".MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bdpaquiz.andersen.stanford.bdpamilwaukeequiz.MyActivity" />
        </activity>

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" >
        </service>

        <activity
            android:name=".DBTest"
            android:label="@string/title_activity_dbtest" >
        </activity>
        <activity
            android:name=".dbactivate"
            android:label="@string/title_activity_dbactivate" >
        </activity>
    </application>

</manifest>
